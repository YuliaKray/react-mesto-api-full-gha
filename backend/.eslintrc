{
  "env": {
      "commonjs": true,
      "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": [
      "error", {
        "allow": [
          "_id"
        ]
      }
    ],
    "arrow-body-style": ["warn", "always"],
    "object-shorthand": ["warn", "always"],
    "no-unused-vars": "warn",
    "no-shadow": ["error", { "allow": ["user", "result"] }]
    }
}